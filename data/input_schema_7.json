{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MLInputData",
  "type": "object",
  "properties": {
    "age": {
      "type": "integer",
      "minimum": 0,
      "maximum": 120,
      "description": "Age in years",
      "default": -1
    },
    "name": {
      "type": "string",
      "description": "Customer name",
      "default": "UNKNOWN",
      "minLength": 1
    },
    "income": {
      "type": "number",
      "minimum": -1,
      "maximum": 10000000,
      "description": "Annual income"
    },
    "credit_score": {
      "type": "number",
      "minimum": 300,
      "maximum": 850,
      "description": "Credit score"
    },
    "gender": {
      "type": "string",
      "enum": ["M", "F", "O"],
      "description": "Gender"
    }
  },
  "required": ["age", "name"],
  "additionalProperties": true
}