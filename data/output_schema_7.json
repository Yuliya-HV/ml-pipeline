{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MLOutputData",
  "type": "object",
  "properties": {
    "prediction": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Fraud probability prediction"
    },
    "confidence": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Model confidence score"
    },
    "risk_category": {
      "type": "string",
      "enum": ["low", "medium", "high"],
      "description": "Risk category"
    },
    "model_version": {
      "type": "string",
      "description": "Model version used"
    }
  },
  "required": ["prediction", "confidence", "risk_category"],
  "additionalProperties": false
}